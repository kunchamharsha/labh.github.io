            crossorigin="anonymous"
            referrerpolicy="no-referrer"
        ></script>
        <link rel="stylesheet" href="/styles/mobile-bank-main.css" />
        <title>{{ title }}</title>
    </head>
    <body>
        <div class="header">
            <div class="logo">
                <img src="/assets/mobile-webview/Frame 2685.png" alt="logo" srcset="">
            </div>
            <div class="go-back d-flex align-items-center justify-content-left">
                <img src="/assets/mobile-webview/Arrow left.png" alt="arrow left">
                <img src="/assets/mobile-webview/fluent_building-bank-16-regular.png" alt="home">
                <div class="details">
                    <div class="h">
                        Bank Details
                    </div>
                    <div class="b">
                        Check and upadate your bank details
                    </div>
                </div>
            </div>
        </div>
        {{ content | safe }}
    </body>
</html>
